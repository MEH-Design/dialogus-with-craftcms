{
  "qualified-headings": true,
  "unique-headings": true,

  //no support for IE < 8
  "box-sizing": false,

  //susy uses floats
  "floats": false,

  //we use it once for box-sizing
  "universal-selector": false,

  //we use border-box
  "box-model": false,

  //we use autoprefixer
  "compatible-vendor-prefixes": false,

  //we use the new h1-6 model
  "font-sizes": false,

  "zero-units": 2
}
